    "def max(num_list):\n",
    "    max = num_list[0]\n",
    "    for i in num_list:\n",
    "        if max <= i:\n",
    "            max = i\n",
    "    return max\n",
    "\n",
    "\n",
    "num_list = [10, 7, 11, 5, 13, 8]\n",
    "\n",
    "print(max(num_list))"
